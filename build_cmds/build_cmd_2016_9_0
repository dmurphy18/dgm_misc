salt pi2 state.sls repo.debian.debian8  pillar='{ "build_dest" : "/srv/debian/2016.9.0/pkgs", "keyid" : "DE57BFBE" , "build_version" : "2016_9_0", "gpg_passphrase" : "fill-in here" , "build_arch" : "armhf" }'


