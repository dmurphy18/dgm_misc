salt c7m state.highstate  pillar='{ "build_dest" : "/srv/redhat/2015.8.12/pkgs", "build_release" : "rhel6", "build_version" : "2015_8_12" }'

salt c7m state.highstate  pillar='{ "build_dest" : "/srv/redhat/2015.8.12/pkgs", "build_release" : "rhel6", "build_version" : "2015_8_12", "build_arch" : "i386" }'

salt c7m state.sls repo.redhat.rhel6  pillar='{ "build_dest" : "/srv/redhat/2015.8.12/pkgs", "keyid" : "DE57BFBE", "build_release" : "rhel6", "build_version" : "2015_8_12" , "gpg_passphrase" : "fill-in here" }'
