## Debian 8

salt d8m state.sls setup.debian.debian8 pillar='{ "build_dest" : "/srv/debian/2015.8.12/pkgs" }'

salt d8m state.sls pkg.salt.2015_8_12.debian8 pillar='{ "build_dest" : "/srv/debian/2015.8.12/pkgs", "build_version" : "2015_8_12" }'

salt d8m state.highstate pillar='{ "build_dest" : "/srv/debian/2015.8.12/pkgs" , "build_version" : "2015_8_12" }'

salt d8m state.sls repo.debian.debian8  pillar='{ "build_dest" : "/srv/debian/2015.8.12/pkgs", "keyid" : "DE57BFBE" , "build_version" : "2015_8_12", "gpg_passphrase" : "fill-in here" }'


